package com.student.manage;

import java.sql.Connection;
import java.sql.PreparedStatement;

public class InsertStudentDao {
	public static boolean InsertIntoDB(Student st)
	{
		try
		{
			Connection con=CP.createConnection();
			String s="insert into Student_manage(id,Name,Standard) values (?,?,?);";
			PreparedStatement pastmt=con.prepareStatement(s);
		}
		catch(Exception e)
		{
			e.printStackTrace();
		}
		
		
	}
}
